# maintance.vitorx86.dev app
# foward to a docker running on the machine, with the maintance app

server {
    listen 80;

    server_name maintance.vitorx86.dev;
    

    access_log /var/log/nginx/nginx.vhost.access.log;
    error_log /var/log/nginx/nginx.vhost.error.log;

    location * {
        proxy_pass http://localhost:8081/;
    }  
}